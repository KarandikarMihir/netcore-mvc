@{
    ViewData["Title"] = "Home Page";
}
@{
    var categories = (List<UserModel>) ViewData["users"]; // Cast the list
}
<div class="container">
    <h1>Home View</h1>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var item in categories)
            {
                <tr>
                    <td>item.id</td>
                    <td>item.name</td>
                    <td>item.email</td>
                </tr>
            }
        </tbody>
    </table>
</div>